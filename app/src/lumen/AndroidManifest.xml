<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application tools:ignore="GoogleAppIndexingWarning">
        <service android:name="de.blinkt.openvpn.core.OpenVPNService">
            <meta-data
                android:name="notification_title"
                android:resource="@string/vpn_notification_title" />
            <meta-data
                android:name="notification_content"
                android:resource="@string/vpn_notification_content" />
            <meta-data
                android:name="notification_icon"
                android:resource="@android:drawable/ic_secure" />
            <meta-data
                android:name="app_package_name"
                android:value="@string/application_id" />
        </service>
    </application>
</manifest>